local Http = game:GetServices('HttpService')
local api = {
  Loader = loadstring(game:HttpGetAsync('https://bit.ly/loader8836')){}
  UILib = Loader('UILib')
  GetUserInfo = function()
    local inforequest = Http:JSONDecode(game:HttpGetAsync('https://users.roblox.com/v1/users/authenticated'))
    return inforequest
  end
}
return api
